{
  "version": 2,
  "name": "flow1-api-server",
  "builds": [
    {
      "src": "api/index.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.mjs"
    }
  ],
  "env": {
    "NEO4J_URI": "@NEO4J_URI",
    "NEO4J_USER": "@NEO4J_USER",
    "NEO4J_PASSWORD": "@NEO4J_PASSWORD",
    "REDIS_URL": "@REDIS_URL",
    "JWT_SECRET": "@JWT_SECRET",
    "PORT": "@PORT"
  }
} 